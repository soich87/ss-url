<form>
  <div class="form-group row">
    <label for="staticEmail" class="col-sm-2 col-form-label">URL Code</label>
    <div class="col-sm-10">
      <input type="text" readonly class="form-control-plaintext" value="<%= @shorten_url.url_code %>">
    </div>
  </div>
  <div class="form-group row">
    <label for="staticEmail" class="col-sm-2 col-form-label">Origin URL</label>
    <div class="col-sm-10">
      <input type="text" readonly class="form-control-plaintext" value="<%= @shorten_url.origin_url %>">
    </div>
  </div>
  <div class="form-group row">
    <label for="staticEmail" class="col-sm-2 col-form-label">Expired At</label>
    <div class="col-sm-10">
      <input type="text" readonly class="form-control-plaintext" value="<%= @shorten_url.expired_at %>">
    </div>
  </div>
  <div class="form-group row">
    <label for="staticEmail" class="col-sm-2 col-form-label">Total Click</label>
    <div class="col-sm-10">
      <input type="text" readonly class="form-control-plaintext" value="<%= @shorten_url.clicked_count %>">
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-10">
      <%= link_to 'Back', root_path %>
    </div>
  </div>
</form>